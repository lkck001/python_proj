Cell[BoxData[
 RowBox[{
  RowBox[{"(*", 
   RowBox[{
    RowBox[{"Define", " ", "the", " ", "acceleration"}], ",", 
    RowBox[{"initial", " ", "velocity"}], ",", 
    RowBox[{"and", " ", "initial", " ", "position"}]}], "*)"}], 
  RowBox[{
   RowBox[{
    RowBox[{"a", "=", 
     RowBox[{"{", 
      RowBox[{"2", ",", 
       RowBox[{"-", "6"}], ",", 
       RowBox[{"-", "4"}]}], "}"}]}], ";"}], "\n", 
   RowBox[{
    RowBox[{"v0", "=", 
     RowBox[{"{", 
      RowBox[{
       RowBox[{"-", "5"}], ",", "1", ",", "3"}], "}"}]}], ";"}], "\n", 
   RowBox[{
    RowBox[{"r0", "=", 
     RowBox[{"{", 
      RowBox[{"6", ",", 
       RowBox[{"-", "2"}], ",", "1"}], "}"}]}], ";"}], "\[IndentingNewLine]", 
   RowBox[{"(*", 
    RowBox[{
    "Define", " ", "the", " ", "velocity", " ", "and", " ", "position", " ", 
     "as", " ", "functions", " ", "of", " ", "a", " ", "symbolic", " ", 
     "variable"}], "*)"}], "\[IndentingNewLine]", 
   RowBox[{
    RowBox[{
     RowBox[{"v", "[", "s_", "]"}], "=", 
     RowBox[{
      RowBox[{"Integrate", "[", 
       RowBox[{"a", ",", "s"}], "]"}], "+", "v0"}]}], ";"}], "\n", 
   RowBox[{
    RowBox[{
     RowBox[{"r", "[", "s_", "]"}], "=", 
     RowBox[{
      RowBox[{"Integrate", "[", 
       RowBox[{
        RowBox[{"v", "[", "s", "]"}], ",", "s"}], "]"}], "+", "r0"}]}], ";"}],
    "\n", 
   RowBox[{"(*", 
    RowBox[{"Define", " ", "the", " ", "intersection", " ", "points"}], 
    "*)"}], "\[IndentingNewLine]", 
   RowBox[{
    RowBox[{"points", "=", 
     RowBox[{"{", 
      RowBox[{
       RowBox[{"{", 
        RowBox[{"0", ",", 
         RowBox[{"-", "12"}], ",", 
         RowBox[{"-", "1"}]}], "}"}], ",", 
       RowBox[{"{", 
        RowBox[{"0", ",", 
         RowBox[{"-", "26"}], ",", 
         RowBox[{"-", "8"}]}], "}"}]}], "}"}]}], ";"}], "\[IndentingNewLine]", 
   RowBox[{"(*", 
    RowBox[{
    "Plot", " ", "the", " ", "trajectory", " ", "and", " ", "the", " ", 
     "intersection", " ", "points"}], "*)"}], "\[IndentingNewLine]", 
   RowBox[{
    RowBox[{"trajectory", "=", 
     RowBox[{"ParametricPlot3D", "[", 
      RowBox[{
       RowBox[{
        RowBox[{"r", "[", "s", "]"}], "/.", " ", 
        RowBox[{"s", "->", "t"}]}], ",", 
       RowBox[{"{", 
        RowBox[{"t", ",", 
         RowBox[{"-", "10"}], ",", "10"}], "}"}], ",", 
       RowBox[{"PlotStyle", "->", "Blue"}], ",", 
       RowBox[{"Axes", "->", "True"}], ",", 
       RowBox[{"AxesLabel", "->", 
        RowBox[{"{", 
         RowBox[{"\"\<X\>\"", ",", "\"\<Y\>\"", ",", "\"\<Z\>\""}], "}"}]}]}],
       "]"}]}], ";"}], "\n", 
   RowBox[{"(*", 
    RowBox[{
    "Create", " ", "a", " ", "3", "D", " ", "plot", " ", "of", " ", "the", 
     " ", "points", " ", "with", " ", "their", " ", "labels"}], "*)"}], 
   "\[IndentingNewLine]", 
   RowBox[{
    RowBox[{"pointPlot", "=", 
     RowBox[{"Graphics3D", "[", 
      RowBox[{"{", 
       RowBox[{"Red", ",", 
        RowBox[{"PointSize", "[", "Large", "]"}], ",", 
        RowBox[{"Point", "[", "points", "]"}], ",", 
        RowBox[{
         RowBox[{
          RowBox[{"Text", "[", 
           RowBox[{
            RowBox[{"Style", "[", "#", "]"}], ",", 
            RowBox[{"1.2", "#"}]}], "]"}], "&"}], "/@", "points"}]}], "}"}], 
      "]"}]}], ";"}], "\[IndentingNewLine]", 
   RowBox[{"(*", 
    RowBox[{
    "Plot", " ", "a", " ", "line", " ", "through", " ", "the", " ", 
     "intersection", " ", "points"}], "*)"}], "\n", 
   RowBox[{
    RowBox[{"linePlot", "=", 
     RowBox[{"Graphics3D", "[", 
      RowBox[{"{", 
       RowBox[{"Green", ",", "Thick", ",", 
        RowBox[{"Line", "[", "points", "]"}]}], "}"}], "]"}]}], ";"}], "\n", 
   RowBox[{"Show", "[", 
    RowBox[{"trajectory", ",", "pointPlot", ",", "linePlot"}], 
    "]"}]}]}]], "Input",
